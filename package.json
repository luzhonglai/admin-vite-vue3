{
  "name": "admin-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev",
    "test": "vite --mode test",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vite build --mode dev",
    "build:pro": "vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint src --fix \"{src,mock}/**/*.{vue,ts,tsx}\"",
    "prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue}\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "axios": "^0.26.0",
    "echarts": "^5.3.0",
    "element-plus": "^2.0.2",
    "eslint": "^7.19.0",
    "husky": "^7.0.4",
    "mockjs": "^1.1.0",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "scss": "^0.2.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.2.25",
    "vue-router": "4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "eslint-config-prettier": "^8.4.0",
    "eslint-define-config": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "lint-staged": "^12.3.4",
    "pinia": "^2.0.11",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "qs": "^6.10.3",
    "sass": "1.26.2",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-icons": "^0.13.1",
    "unplugin-vue-components": "^0.17.20",
    "vite": "^2.8.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.20.2",
    "vite-plugin-restart": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue-tsc": "^0.29.8",
    "web-storage-cache": "^1.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write .",
      "eslint src --fix --ext  \"{src,mock}/**/*.{vue,ts,tsx}\""
    ]
  },
  "engines": {
    "node": ">=14"
  }
}
